# editorconfig.org
# go to editorconfig,org to download a plugin if you editor of choice doesn't support an .editorconfig file
 
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[.md]
trim_trailing_whitespace = false

